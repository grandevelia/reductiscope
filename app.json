{
  "expo": {
    "name": "reductiscope",
    "slug": "reductiscope",
    "scheme": "reductiscope",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.15",
    "orientation": "portrait",
    "icon": "./assets/reductiscope_logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.theseusllc.reductiscope",
      "supportsTablet": true,
      "associatedDomains": [
        "applinks:reductiscope.com"
      ],
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "192.168.1.5": {
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true
            }
          }
        }
      }
    },
    "description": ""
  }
}